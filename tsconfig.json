{
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "target": "es6",
        "outDir": "out",
        "sourceMap": true,
        "rootDir": "./",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "noUnusedLocals": true,
        "noFallthroughCasesInSwitch": true,
        "strict": true
    },
    "exclude": [
        ".vscode-test",
        "node_modules"
    ]
}