{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": false,
        "arrow-parens": false,
        "triple-equals": false,
        "no-default-export": true,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "max-line-length": 140
    },
    "rulesDirectory": []
}